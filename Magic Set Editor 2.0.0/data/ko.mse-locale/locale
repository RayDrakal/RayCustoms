############################################################## Magic
package:
	magic.mse-game:
		# Card fields
		name:		이름
		cc:			마나・코스트
		type:		카드 종류
		p/t:		파워/터프니스
		rarity:		레어리티
		card name:	카드명
		
		# Set info
		
		# descriptions/help text
		
	magic-mana-*.mse-symbol-font:
		# note: reminder/shortcut must start with a space, otherwise it is used as a shortcut
		#       so typing T *anywhere* would insert a symbol (which would be bad)
		menu item T:			탭・기호(&T)	 T
		menu item Q:			&Untap symbol	 Q
		menu item W:			하얀색 마나(&W)	 W
		menu item U:			파랑색 마나(&U)	 U
		menu item B:			검은색 마나(&B)	 B
		menu item R:			빨간색 마나(&R)	 R
		menu item G:			초록색 마나(&G)	 G
		menu item S:			얼음색 마나(&S)	 S
